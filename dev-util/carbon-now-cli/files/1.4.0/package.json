{
	"name": "carbon-now-cli",
	"version": "1.4.0",
	"description": "Beautiful images of your code — from right inside your terminal.",
	"license": "MIT",
	"repository": "mixn/carbon-now-cli",
	"author": {
		"name": "Miloš Sutanovac",
		"email": "sutanovac.milos@gmail.com",
		"url": "https://mixn.io"
	},
	"bin": {
		"carbon-now": "cli.js"
	},
	"engines": {
		"node": ">=8.3"
	},
	"scripts": {
		"test": "xo && ava --serial",
		"ava": "ava --serial"
	},
	"files": [
		"cli.js",
		"src"
	],
	"keywords": [
		"cli",
		"cli-app",
		"carbon-now",
		"carbon-now-sh",
		"carbon-now-sh-cli",
		"carbon",
		"screenshot",
		"beautiful"
	],
	"dependencies": {
		"chalk": "^2.4.1",
		"clipboardy": "^1.2.3",
		"del": "^3.0.0",
		"file-exists": "^5.0.1",
		"file-extension": "^4.0.5",
		"get-stdin": "^6.0.0",
		"globby": "^8.0.1",
		"inquirer": "^6.2.1",
		"jsonfile": "^5.0.0",
		"listr": "^0.14.3",
		"lodash": "^4.17.11",
		"meow": "^5.0.0",
		"nanoid": "^2.0.0",
		"opn": "^5.4.0",
		"puppeteer": "^1.11.0",
		"query-string": "^6.2.0",
		"tempy": "^0.2.1",
		"terminal-image": "^0.1.2",
		"update-notifier": "^2.5.0"
	},
	"devDependencies": {
		"ava": "^0.25.0",
		"child-process-promise": "^2.2.1",
		"execa": "^1.0.0",
		"husky": "^1.0.0-rc.14",
		"xo": "^0.21.1"
	},
	"xo": {
		"ignores": [
			"test/test-dummies/*.js"
		],
		"globals": [
			"document"
		],
		"rules": {
			"no-warning-comments": 0,
			"promise/prefer-await-to-then": 0
		}
	},
	"husky": {
		"hooks": {
			"pre-push": "npm test"
		}
	}
}
