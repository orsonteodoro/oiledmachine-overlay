}

repo_latest_version_get_hyphen_pv() {
	echo $(wget -q -O - \
"https://sourceforge.net/projects/hunspell/rss?path=/Hyphen" \
		| grep -E -o -e "hyphen-[0-9\.]+\.tar\.gz" \
		| sed -r -e "s|-([0-9]\.[0-9])\.tar\.gz|-\1.0.tar.gz|g" \
		| sed -e "s|hyphen-||" -e "s|\.tar\.gz||" \
		| esort \
		| tr " " "\n" \
		| uniq \
		| tail -n 1)
}

