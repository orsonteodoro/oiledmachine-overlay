repo_latest_version_get_boinc_pv() {
	echo $(git ls-remote --tags \
https://github.com/BOINC/boinc.git \
		| cut -f 3- -d "/" \
		| grep -v -G -e "[\^]" \
		| grep -F -e "client_release" \
		| cut -f 3 -d "/" \
		| grep -v -F -e "OSX_notices_hotfix" \
		| esort \
		| tr " " "\n" \
		| tail -n 1)
}

