		| sed -r -e "s#(Release-|rel-|release-)##g" \
			-e "s|-RC|_rc|g" \
			-e "s|Beta|_beta|g" \
			-e "s|-public||g" \
			-e "s|-MonoDevelop||g" \
			-e "s|monodevelop-|2.8.1|g" \
			-e "s|Release([0-9])|\1|g" \
			-e "s|mondevelop-|2.8.1|g" \
			-e "s|||g" \
		| grep -v -F -e "2.0oob-0619" \
		| grep -v -F -e "-NuGet.Core_Only" \
		| esort \
		| tr " " "\n" \
		| uniq \
		| tail -n 1)
	echo "${pv}"
}

